ls -a && echo hello
ls -a || echo hello
ls -a; echo world || echo man && echo linux; echo Oi!
ls || echo boy && echo man
git status && echo name || echo handsomeBoy
echo hello || echo bill || echo gates || echo Microsoft
echo hello && echo steve && echo jobs && echo Apple
mkdir testable && rm -r testable
mkdir testable && rm -r testable || touch blah && rm blah
echo magic && echo hogwarts || echo speed && history
mkdir && ls Bill && ps
echo A && echo B
echo A || echo B
echo A; echo B
ls -j && echo A
ls -j || echo A
ls -j; echo A
ls -j && echo A || echo B
echo A || echo B || echo C
echo A || echo B && echo C
echo A && echo B && echo C
exit
